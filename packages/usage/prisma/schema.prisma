generator client {
  provider = "prisma-client-js"
}

generator next_crud_generator {
  provider = "npx next-crud-generator"
  output   = "../src"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id    String @id @default(cuid())
  email String @unique
  name  String
}
